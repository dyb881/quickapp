{
  "package": "com.application.demo",
  "name": "quickapp",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "1020",
  "icon": "/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.websocketfactory"
    },
    {
      "name": "system.device"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.fetch"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "log",
    "designWidth": 1920
  },
  "router": {
    "entry": "pages/index",
    "pages": {
      "pages/index": {
        "component": "index"
      },
      "pages/auth": {
        "component": "index"
      }
    }
  },
  "display": {
    "titleBar": false,
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "orientation": "landscape",
    "backgroundColor": "#001334",
    "pages": {
      "pages/index": {
        "titleBarText": "快应用APP"
      },
      "pages/auth": {
        "titleBarText": "授权登录"
      }
    }
  },
  "trustedSslDomains": []
}
